<ImageButton@Button>:
    source: None

    Image:
        source: root.source
        pos: root.pos
        size: root.size
MDScreen:
    name: "main"
    id: main
    MDFloatLayout:
        md_bg_color: 191/255, 172/255, 224/255, 1
        Image:
            pos_hint: {'center_x': 0.5, 'center_y': 0.93}
            source: "topBar.png"

        Label:
            text: "나는 어떤 동물상일까요?"
            font_name: "Hancom Gothic Regular.ttf"
            font_size: 25
            pos_hint: {'center_x':0.5, 'center_y':0.79}

        ImageButton:
            id: manIcon_Button
            source: "manIcon.png"
            size_hint: (None, None)
            pos_hint: {'center_x': 0.35, 'center_y': 0.68}
            on_press: app.manIcon_Button_clicked()
            background_color: 0,0,0,0


        ImageButton:
            id: womanIcon_Button
            source: "womanIcon.png"
            size_hint: (None, None)
            pos_hint: {'center_x': 0.65, 'center_y': 0.68}
            on_press: app.womanIcon_Button_clicked()
            background_color: 0,0,0,0

        Image:
            pos_hint: {'center_x': 0.5, 'center_y': 0.35}
            source: "imageBoxOut.png"

        Image:
            size_hint: (1,1)
            pos_hint: {'center_x': 0.5, 'center_y': 0.35}
            source: "imageBoxIn.png"


        ImageButton:
            id: imageUpload_Button
            source: "imageUpload.png"
            size_hint: (0.3, 0.05)
            pos_hint: {'center_x': 0.5, 'center_y': 0.35}
            on_release:
                root.manager.current = "gallary"
            background_color: 0,0,0,0

        Label:
            id: genderClick
            opacity: 0
            text: ""

