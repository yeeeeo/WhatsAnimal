<LoadingScreen>:
    name: "loadingScreen"
    MDFloatLayout:
        md_bg_color: 191/255, 172/255, 224/255, 1
        Image:
            pos_hint: {'center_x': 0.5, 'center_y': 0.93}
            source: "topBar.png"


        Image:
            id: loadingImage
            pos_hint: {'center_x': 0.5, 'center_y': 0.5}
